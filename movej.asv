function movej(angles1,angles2,step)
    [t11,t12,t13,t14,t15] = angles
    for time = 0:1:step
        t1 = t11 + ((t21-t11)/step)*time;
        t2 = t12 + ((t22-t12)/step)*time;
        t3 = t13 + ((t23-t13)/step)*time;
        t4 = t14 + ((t24-t14)/step)*time;
        t5 = t15 + ((t25-t15)/step)*time;
        display_robot(t1,t2,t3,t4,t5);
        drawnow;
    end
end

